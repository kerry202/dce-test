<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!-- 导航栏 -->
        <cn.dagongniu.oax.customview.tabview.TabView
            android:id="@+id/tabView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            app:tab_imageViewHeight="22dp"
            app:tab_imageViewWidth="22dp"
            app:tab_tabViewHeight="55dp"
            app:tab_textViewSelColor="@color/df_theme"
            app:tab_textViewSize="12sp"
            app:tab_textViewUnSelColor="@color/df_font_gray" >
        </cn.dagongniu.oax.customview.tabview.TabView>

    </com.zhy.autolayout.AutoRelativeLayout>
</RelativeLayout>